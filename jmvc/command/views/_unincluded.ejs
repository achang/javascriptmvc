<table id='<%= type+'s' %>' width='100%' cellspacing='0px' cellpadding='0px'>
    <tr>
	<% var count = 0;
	for(var j=0; j<files.length; j++) { %>
		<% if(!included || included.indexOf(files[j]) == -1) { 
			if(count%4==0) { 
				count = 0; %>
				</tr><tr>
			<% } %>
				<td class='<%= type %>'><input type='checkbox' /> <%= files[j] %></td>
			<% count++; %>
	<% } } %>
    </tr>
</table>