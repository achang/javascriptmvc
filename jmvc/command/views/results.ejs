<div class='include_type'>
    <h4>views</h4>
    <div class='left'>(</div>
    <div class='includes'>
    <% var partial = (new MVC.View({absolute_url: 'command/views/_included.ejs'}).render({
    	iter: included.views, 
    	end_with_comma: false,
    	type: 'view'})); %>
    <%= partial %>
    </div>
    <span>)</span>
    <div class='clear'></div>
    <div class='indent'>
	    <% var partial = (new MVC.View({absolute_url: 'command/views/_unincluded.ejs'}).render({
	    	type: 'view',
	    	included: included.views,
	    	files: files.views})); %>
	    <%= partial %>
        <p class='gray'>Views are HTML template files that help clean HTML from your JavaScript.  
        <a href='http://javascriptmvc.com/learningcenter/view/index.html'>Learn more</a></p>
    </div>
</div>

<div class='include_type'>
    <h4>plugins</h4>
    <div class='left'>(</div>
    <div class='includes'>
    <% var partial = (new MVC.View({absolute_url: 'command/views/_included.ejs'}).render({
    	iter: plugins_in_app_file, 
    	end_with_comma: false,
    	type: 'plugin'})); %>
    <%= partial %>
    </div>
    <span>)</span>
    <div class='clear'></div>
    <ul id='plugins'>
	<% for(var j=0; j<files.plugins.length; j++) { %>
		<% var disabled = true;
		if(included.plugins.indexOf(files.plugins[j]) == -1)
			disabled = false; %>
		
		<li class='plugin'><input type='checkbox' <%= disabled? "disabled='true'": "" %> <%= disabled? "checked='true'": "" %> /> 
		<% if(MVC.PluginDescriptions[files.plugins[j]]){ %>
			<a href='<%= MVC.PluginDescriptions[files.plugins[j]].url %>'><%= files.plugins[j] %></a> - 
			<%= MVC.PluginDescriptions[files.plugins[j]].description %>
		<% } else { %>
			<%= files.plugins[j] %>
		<% } %>
		</li>
	<% } %>
	</ul>
</div>